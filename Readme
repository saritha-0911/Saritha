#include<studio.h>

int main ()
{
 int div,sub;
 printf("Enter the class obtained by the student:");
 scanf(" %d", &div);
 
 printf("Enter the number of subjects in which he has failed:");
 scanf("%d",&sub);

 switch(div)
 {
   // for first class students
  case 1:
        if (sub<=3)
           printf("He got the grace of %d marks",5*sub);
        else
           printf("He didn't get any grace");

 return 0;
}
